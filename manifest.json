{ 
	"name": "天凉右键", 
	"description": "天凉右键", 
	"version": "0.1", 
	"permissions": [
		"contextMenus", 
		"browsingData", 
		"commands", 
		"nativeMessaging",
		"storage",
		"http://dict.youdao.com/"
	], 
	"background": { 
		"scripts": ["menu.js", "libs/socket.io-1.3.6.js"]
	}, 
	"browser_action": {
		"default_icon" : "icon.png",
		"default_popup": "popup.html"
	},	
   "content_security_policy": "script-src 'self'; font-src 'self'; img-src 'self' data:; style-src 'self' 'unsafe-inline'; connect-src *; default-src 'none'",
	"manifest_version": 2,
	"commands": {
		"clearCache": {
			"suggested_key": {
				"windows": "Alt+C"
			},
			"description": "清除缓存"
		},
		"clearCookie": {
			"suggested_key": {
				"windows": "Alt+K"
			},
			"description": "清除Cookie"
		},
		"clearLocalStrorage": {
			"suggested_key": {
				"windows": "Alt+L"
			},
			"description": "清除LocalStorage"
		},
		"startNativeApp": {
			"suggested_key": {
				"windows": "Alt+N"
			},
			"description": "启动本地程序"
		}
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "*://*/*"],
			"js": [
				"libs/jquery-2.1.4.min.js", 
				"scripts/content.js"
			],
			"css": [

			]
		}
	]
} 